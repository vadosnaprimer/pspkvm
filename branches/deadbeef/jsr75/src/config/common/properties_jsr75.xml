<!-- System-independent JSR 75 properties required by the specification -->
<configuration>
<properties>
<!-- System  properties (visible via System.getProperty() -->
  <property Key="microedition.io.file.FileConnection.version"
      Value="1.0"
      Scope="system"
      Comment="The version of FileConnection API."/>

  <property Key="microedition.pim.version"
      Value="1.0"
      Scope="system"
      Comment="The version of PIM API."/>

   <property Key="fileconn.dir.roots.names"
      Value=""
      Scope="system"
      Callout="getLocalizedMountedRoots"
      Comment="Localized names corresponding to roots returned by FileSystemRegistry.listRoots() method."/>

  <property Key="fileconn.dir.private.name"
      Value=""
      Scope="system"
      Callout="getLocalizedPrivateDir"
      Comment="The localized name of private directory for MIDlet suite."/>

<!-- Internal properties (visible via Configuration.getProperty() -->
</properties>
</configuration>
