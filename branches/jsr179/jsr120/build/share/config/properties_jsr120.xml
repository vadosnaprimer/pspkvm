<!-- System-independent JSR 205 properties required by the specification -->
<configuration>
  <properties>
    <!-- SYSTEM PROPERTIES -->
    <!-- Visible from System.getProperty() -->

    <property Key="wireless.messaging.version"
      Value="1.1"
      Scope="system"
      Comment="WMA version number."/>

    <property Key="wireless.messaging.sms.smsc"
      Value="+17815511212"
      Scope="system"
      Comment="Default SMS Service Center address."/>
    
    <!-- INTERNAL PROPERTIES -->
    <!-- Visible from Configuration.getProperty() -->
    <property Key="com.sun.midp.io.j2me.wma.DatagramHost"
      Value="wmadatagramhost"
      Comment="The datagram host name for wma emulated messages."/>
    
    <property Key="com.sun.midp.io.j2me.sms.DatagramPortIn"
      Value="11100"
      Scope="internal"
      Comment="The datagram port number for inbound SMS messages."/>
    
    <property Key="com.sun.midp.io.j2me.sms.DatagramPortOut"
      Value="11101"
      Scope="internal"
      Comment="The datagram port number of outbound SMS messages."/>
    
    <property Key="com.sun.midp.io.j2me.cbs.DatagramPortIn"
      Value="22200"
      Scope="internal"
      Comment="The datagram port number for inbound CBS messages."/>
    
    <property Key="com.sun.midp.io.j2me.sms.PhoneNumber"
      Value="+5551212"
      Scope="internal"
      Comment="The phone number for outbound messages from the SMS emulator."/>
    
  </properties>
</configuration>
