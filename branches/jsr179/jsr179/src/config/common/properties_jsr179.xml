<!-- System-independent JSR 179 properties required by the specification -->
<configuration>
<properties>
<!-- SYSTEM PROPERTIES -->
<!-- Visible from System.getProperty() -->
  <property Key="microedition.location.version"
      Value="1.0.1"
      Scope="system"
      Comment="Version number of the Location API"/>
<!-- INTERNAL PROPERTIES -->
<!-- Visible from Configuration.getProperty() -->
  <property Key="com.sun.j2me.location.OrientationSupported"
      Value="false"
      Scope="internal"
      Comment="True when orientation is supported, false otherwise."/>
  <property Key="com.sun.j2me.location.ProximitySupported"
      Value="true"
      Scope="internal"
      Comment="True when proximity monitoring is supported, false otherwise."/>
  <property Key="com.sun.j2me.location.CreateLandmarkStoreSupported"
      Value="true"
      Scope="internal"
      Comment="True when landmark store creation is supported, false otherwise."/>
  <property Key="com.sun.j2me.location.DeleteLandmarkStoreSupported"
      Value="true"
      Scope="internal"
      Comment="True when landmark store deletion is supported, false otherwise."/>
  <property Key="com.sun.j2me.location.DeleteCategorySupported"
      Value="true"
      Scope="internal"
      Comment="True when category deletion is supported, false otherwise."/>
  <property Key="com.sun.j2me.location.ResetTimeout"
      Value="10"
      Scope="internal"
      Comment="Timeout value in seconds for LocationProvider.reset() method."/>
</properties>
</configuration>
