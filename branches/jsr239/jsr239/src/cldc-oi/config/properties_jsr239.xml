<configuration>
<properties>
<!-- System  properties (visible via System.getProperty() -->
  <property Key="microedition.khronos.egl.version"
      Value="1.1"
      Scope="system"
      Comment="The version of EGL API."/>
      
  <property Key="microedition.khronos.opengles.version"
      Value="1.1"
      Scope="system"
      Comment="The version of OpenGL ES API."/>

<!-- Internal properties (visible via Configuration.getProperty() -->
  <property Key="jsr239.noqueue"
      Value="false"
      Scope="internal"
      Comment="Flush queue after each command."/>
      
  <property Key="jsr239.supportsEGL11"
      Value="true"
      Scope="internal"
      Comment="Whether native library supports EGL 1.1."/>

  <property Key="jsr239.supportsGL11"
      Value="true"
      Scope="internal"
      Comment="Whether native library supports GL 1.1."/>

  <property Key="jsr239.supports_OES_query_matrix"
      Value="true"
      Scope="internal"
      Comment="Whether the extension is supported."/>
      
  <property Key="jsr239.supports_OES_draw_texture"
      Value="true"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_matrix_palette"
      Value="true"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_texture_cube_map"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_texture_env_crossbar"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_texture_mirrored_repeat"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>
      
  <property Key="jsr239.supports_OES_blend_subtract"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_blend_func_separate"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_blend_equation_separate"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>
      
  <property Key="jsr239.supports_OES_stencil_wrap"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_extended_matrix_palette"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

  <property Key="jsr239.supports_OES_framebuffer_object"
      Value="false"
      Scope="internal"
      Comment="Whether the extension is supported."/>

</properties>
</configuration>
