<!-- System-specific JSR 75 properties required by the specification -->
<configuration>
<properties>
<!-- System  properties (visible via System.getProperty() -->
  <property Key="fileconn.dir.private"
      Value="file:///Private/"
      Scope="system"
      Comment="The private work directory for MIDlet suite."/>

  <property Key="file.separator"
      Value="\\"
      Scope="system"
      Comment="The file separator character(s) for the underlying platform."/>

<!-- Internal properties (visible via Configuration.getProperty() -->
  <property Key="PIMRootDir"
      Value="file:///PIMdb/"
      Scope="internal"
      Comment="The root directory that contains PIM data."/>

  <property Key="DefaultContactList"
      Value="Default_contact"
      Scope="internal"
      Comment="The directory name of the default contact list."/>
		    
  <property Key="DefaultEventList"
      Value="Default_event"
      Scope="internal"
      Comment="The directory name of the default event list."/>
				
  <property Key="DefaultTodoList"
      Value="Default_todo"
      Scope="internal"
      Comment="The directory name of the default todo list."/>							    

  <property Key="file.linebreak"
      Value="\r\n"
      Scope="internal"
      Comment="The which terminates a line in text files."/>
</properties>
</configuration>
