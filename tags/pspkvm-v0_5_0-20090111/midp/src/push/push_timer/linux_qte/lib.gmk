######################################################################
#
# Subsystem's Platform Configuration file
#
######################################################################

# vpath for this platform specific files
#
vpath % $(SUBSYSTEM_DIR)/push/push_timer/$(PLATFORM)/native

# Additional native files for the platform
#
SUBSYSTEM_PUSH_TIMER_NATIVE_FILES += \
    midpTimerImpl.cpp

# Header files for the platform
SUBSYSTEM_PUSH_TIMER_EXTRA_INCLUDES += \
    -I$(SUBSYSTEM_DIR)/push/push_timer/$(PLATFORM)/include \
    -I$(SUBSYSTEM_DIR)/push/push_timer/$(PLATFORM)/native

# Uses the global MIDP_OUTPUT_DIR as the outcome directory
#
$(MIDP_OUTPUT_DIR)/moc_%.cpp: $(SUBSYSTEM_DIR)/push/push_timer/$(PLATFORM)/native/%.h
	@echo Creating ... $@ using MOC $(LOG_ONLY)
	@echo +++ $(QTOPIA_SDK_DIR)/bin/moc -i $< -o $@ $(LOG_ONLY)
	@(cd $(MIDP_OUTPUT_DIR); $(QTOPIA_SDK_DIR)/bin/moc -i $< -o $@) $(LOG)

$(OBJ_DIR)/midpTimerImpl.o: $(MIDP_OUTPUT_DIR)/moc_midpTimerImpl.cpp
