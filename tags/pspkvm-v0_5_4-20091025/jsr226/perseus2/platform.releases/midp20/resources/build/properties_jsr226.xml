<!-- System-independent JSR 226 properties required by the specification -->
<configuration>
  <properties>
    <!-- SYSTEM PROPERTIES -->
    <!-- Visible from System.getProperty() -->

    <property Key="microedition.m2g.version"
      Value="1.1"
      Scope="system"
      Comment="Version of the Scalable 2D Vector Graphics API supported by the device."/>

    <property Key="microedition.m2g.svg.version"
      Value="1.1"
      Scope="system"
      Comment="The version of SVG that is supported by the underlying implementation."/>

    <property Key="microedition.m2g.svg.baseProfile"
      Value="tiny"
      Scope="system"
      Comment="The base profile of SVG that is supported by the underlying implementation."/>

    <!-- INTERNAL PROPERTIES -->
    <!-- Visible from Configuration.getProperty() -->
    
  </properties>
</configuration>
