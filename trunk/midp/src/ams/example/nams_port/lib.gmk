######################################################################
# Module's Component Configuration file
#
# Remember: It is up to the module itself to impose rules of
# how to compile the options. All the global Makefile needs
# is the final def. of Java and native file lists and some
# other def. (like VPATH)
######################################################################

# vpath for ( nams_port ) module
#

ifeq ($(TARGET_PLATFORM), javacall)
    vpath % $(SUBSYSTEM_AMS_DIR)/example/nams_port/$(TARGET_PLATFORM)/native

    SUBSYSTEM_AMS_EXTRA_INCLUDES += \
        -I$(SUBSYSTEM_AMS_DIR)/example/nams_port/$(TARGET_PLATFORM)/native

    AMS_RUNNAMS_NATIVE_FILES += nams.c
else
    vpath % $(SUBSYSTEM_AMS_DIR)/example/nams_port/default/native

    SUBSYSTEM_AMS_EXTRA_INCLUDES += \
        -I$(SUBSYSTEM_AMS_DIR)/example/nams_port/$(TARGET_PLATFORM)/native
endif

SUBSYSTEM_AMS_EXTRA_INCLUDES += \
    -I$(SUBSYSTEM_AMS_DIR)/example/nams/include
	
# Don't include exe files to MIDP source file list
# since we want to link different obj files for different exe
#

# Native files for the ( nams_port ) module
#
ifeq ($(TARGET_VM), cldc_vm)

AMS_RUNNAMS_NATIVE_FILES += \
    runNams_md.c
	
AMS_REMOVEMIDLET_NATIVE_FILES += \
    removeMidlet_md.c

AMS_LISTMIDLET_NATIVE_FILES += \
    listMidlets_md.c
					
AMS_FILEINSTALLER_NATIVE_FILES += \
    fileInstaller_md.c
					
endif
