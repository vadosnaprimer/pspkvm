vpath % $(SUBSYSTEM_LFJPORT_DIR)/linux_qte/native

SUBSYSTEM_LCDLF_NATIVE_FILES += \
    lfjport_qte_export.cpp \
    lfjport_qte_mainwindow.cpp \
    lfjport_qte_mscreen.cpp

# Rules to pre-process Qt files with the Meta Object Compiler
#
$(MIDP_OUTPUT_DIR)/moc_%.cpp: $(SUBSYSTEM_LFJPORT_DIR)/linux_qte/native/%.h
	@echo Creating ... $@ using MOC $(LOG_ONLY)
	@echo +++ cd $(MIDP_OUTPUT_DIR)\; \
	  $(QTOPIA_SDK_DIR)/bin/moc -i $< -o $@ $(LOG_ONLY)
	@(cd $(MIDP_OUTPUT_DIR); \
	  $(QTOPIA_SDK_DIR)/bin/moc -i $< -o $@) $(LOG)

$(OBJ_DIR)/lfjport_qte_mainwindow.o: $(MIDP_OUTPUT_DIR)/moc_lfjport_qte_mainwindow.cpp
$(OBJ_DIR)/lfjport_qte_mscreen.o: $(MIDP_OUTPUT_DIR)/moc_lfjport_qte_mscreen.cpp

$(OBJ_DIR)/lfjport_qte_mainwindow.o: $(MIDP_OUTPUT_DIR)/moc_qteapp_mainwindow.cpp
